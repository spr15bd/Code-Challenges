function permAlone(str) {
  let perms = 0
  let charArray = [] 
  for (let i=0; i<str.length; i++) {
    charArray[i] = str.charAt(i)
    perms++
  }
  console.log(charArray)

  findPerms() {
    // 012 (aab), 021 (aba), 102 (aab), 120 (aba), 201 (baa), 210 (baa)
  }

  // loop over permutations, omit from final array if a permutation repeats a char one after the other, leaving aba and aba

  return perms
}
permAlone('aab')
