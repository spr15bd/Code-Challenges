function updateInventory(arr1, arr2) {
    const curInv = [];
    for(let i = 0; i < arr1.length; i++){
        curInv.push(arr1[i]);
    };
    const newInv = [];
    for(let i = 0; i < arr2.length; i++){
        newInv.push(arr2[i]);
    };
    console.log("CURINV ", curInv)
    arr2.forEach(item=>{
        console.log("arr[2] desc: ", item[1])
        if (curInv.find(i=>i[1]===item[1])) {
            //let itemToAdd = arr1.find(currItem=>currItem[1]===item[1])
            console.log( 
              "itemToAdd: ", item[1]
            , " amount to add: ", item[0]
            , "item to add to: ", curInv.find(i=>i[1]===item[1]))
            let itemToAddTo = curInv.find(i=>i[1]===item[1])
            itemToAddTo[0] += item[0]
        } else {
            console.log(item[1], " not previously added, adding")
            curInv.push([item[0], item[1]])
            console.log("curInv: ", curInv)
        }
    })
    console.log("ci len ", curInv.length)
    return curInv;
}
// Example inventory lists
var curInv = [
    [21, "Bowling Ball"],
    [2, "Dirty Sock"],
    [1, "Hair Pin"],
    [5, "Microphone"]
];
var newInv = [
    [2, "Hair Pin"],
    [3, "Half-Eaten Apple"],
    [67, "Bowling Ball"],
    [7, "Toothpaste"]
];

updateInventory(curInv, newInv);
